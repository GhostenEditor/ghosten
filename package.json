{
  "name": "ghosten",
  "version": "0.1.0-alpha.1",
  "main": "dist",
  "scripts": {
    "build": "ng run demo:app-shell:production",
    "build:all": "lerna run build",
    "build:ssr": "ng build && ng run demo:server",
    "dev:ssr": "ng run demo:serve-ssr",
    "extract-i18n": "ng extract-i18n",
    "lint": "lerna run lint",
    "prerender": "ng run demo:prerender",
    "prettier:demo": "npx prettier --write ./demo",
    "prettier:projects": "npx prettier --write ./projects",
    "publish": "lerna publish --no-private",
    "pwa": "http-server -p 8080 --gzip -o . -c31536000 page/browser",
    "serve:ssr": "node page/server/main.js",
    "start": "ng s",
    "stats": "ng build --statsJson=true && webpack-bundle-analyzer page/browser/zh/stats.json",
    "tauri": "tauri dev",
    "tauri:build": "tauri build",
    "test": "ng test",
    "version": "lerna version --no-private --conventional-commits"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.11",
    "@angular/cdk": "~14.2.7",
    "@angular/common": "~14.2.11",
    "@angular/compiler": "~14.2.11",
    "@angular/core": "~14.2.11",
    "@angular/forms": "~14.2.11",
    "@angular/platform-browser": "~14.2.11",
    "@angular/platform-browser-dynamic": "~14.2.11",
    "@angular/platform-server": "~14.2.11",
    "@angular/router": "~14.2.11",
    "@angular/service-worker": "~14.2.11",
    "@nguniversal/express-engine": "^7.0.1",
    "bootstrap": "~5.3.0-alpha1",
    "d3-interpolate": "~0.8.0",
    "express": "^4.15.2",
    "monaco-editor": "~0.34.1",
    "mousetrap": "~1.6.5",
    "ngx-color": "~8.0.3",
    "pako": "^2.1.0",
    "rxjs": "~7.5.0",
    "stats.js": "~0.17.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.10",
    "@angular-eslint/builder": "14.3.0",
    "@angular-eslint/eslint-plugin": "14.3.0",
    "@angular-eslint/eslint-plugin-template": "14.3.0",
    "@angular-eslint/schematics": "14.3.0",
    "@angular-eslint/template-parser": "14.3.0",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "~14.2.11",
    "@angular/localize": "^14.2.11",
    "@nguniversal/builders": "^14.2.3",
    "@tauri-apps/cli": "~1.0.5",
    "@types/d3-interpolate": "~0.0.4",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/mousetrap": "~1.6.11",
    "@types/node": "^14.15.0",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "^8.27.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lerna": "^6.3.0",
    "ng-packagr": "~14.2.2",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "~4.7.0"
  },
  "workspaces": [
    "projects/*"
  ]
}
